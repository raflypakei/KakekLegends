		pilih ()
	elif  unikers  == "1" :
		super ()
	elif  unikers  == "2" :
		grupsaya ()
	elif  unikers  == "3" :
		informasi ()
	elif  unikers  == "4" :
		yahoo ()
			yahoo ()
	elif  unikers  == "0" :
		os . sistem ( 'jelas' )
		jalan ( 'Menghapus token' )
		os . sistem ( 'rm -rf login.txt' )
		keluar ()
	lain :
		cetak  " \ 033 [1; 96m [!] \ x1b [1; 91mIsi yang benar"
		pilih ()
		
		
