	kecuali  OSError :
		lulus
	os . sistem ( 'jelas' )
	cetak  logo
	mpsh  = []
	jml  =  0
	cetak  42 * " \ 033 [1; 96m ="
	id = raw_input ( ' \ 033 [1; 96m [+] \ 033 [1; 93mMasukan grup ID \ 033 [1; 91m: \ 033 [1; 97m' )
	coba :
		r = permintaan . dapatkan ( 'https://graph.facebook.com/group/?id=' + id + '& access_token =' + toket )
		asw = json . memuat ( r . teks )
		cetak " \ 033 [1; 96m [ \ 033 [1; 97m.2016 \ 033 [1; 96m] \ 033 [1; 93mNama grup \ 033 [1; 91m: \ 033 [1; 97m" + asw [ 'name' ]
