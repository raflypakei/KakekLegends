def  acak ( x ):
    w  =  'mhkbpcP'
    d  =  ''
    untuk  saya  dalam  x :
        d  + =  '!' + w [ acak . Randand ( 0 , len ( w ) - 1 )] + i
    return  cetak ( d )
    
    
def  cetak ( x ):
    w  =  'mhkbpcP'
    untuk  saya  dalam  w :
        j  =  w . indeks ( i )
        x =  x . ganti ( '!% s' % i , ' \ 033 [% s; 1m' % str ( 31 + j ))
    x  + =  ' \ 033 [0m'
    x  =  x . ganti ( '! 0' , ' \ 033 [0m' )
    sys . stdout . tulis ( x + ' \ n ' )
	

def  jalan ( z ):
	untuk  e  in  z  +  ' \ n ' :
		sys . stdout . tulis ( e )
		sys . stdout . flush ()
		waktu . tidur ( 0,05 )
		
		
