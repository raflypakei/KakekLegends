			mail  =  z [ 'email' ]
			yahoo  =  ulang . kompilasi ( r '@. *' )
			otw  =  yahoo . cari ( mail ). grup ()
			if  'yahoo.com'  dalam  otw :
				br . buka ( "https://login.yahoo.com/config/login?.src=fpctx&.intl=id&.lang=id-ID&.done=https://id.yahoo.com" )
				br . _factory . is_html  =  Benar
